	Date-Added = {2020-02-03 18:32:40 +0000},
	Date-Modified = {2020-02-03 18:33:09 +0000},
	Doi = {https://doi.org/10.1016/j.visres.2018.09.003},
	Isbn = {0042-6989},
	Journal = {Vision Research},
	Keywords = {Structure-from-motion; Stereokinetic; Maximal rigidity; Minimal deformation; Slow and smooth; Motion integration},
	Pages = {53--59},
	Title = {A preference for minimal deformation constrains the perceived depth of a stereokinetic stimulus},
	Ty = {JOUR},
	Url = {http://www.sciencedirect.com/science/article/pii/S0042698918302013},
	Volume = {153},
