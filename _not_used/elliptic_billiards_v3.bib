	Issn = {0010-4485},
	Journal = {Computer-Aided Design},
	Keywords = {Dynamic geometry, Locus computation, Parametric polynomial systems, Gr{\"o}bnerCover algorithm},
	Pages = {22 - 33},
	Title = {An algebraic taxonomy for locus computation in dynamic geometry},
